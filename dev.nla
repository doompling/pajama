def_e print_int(int Int)
def_e print_bytes(byte_ptr BytePtr, int Int)

class Str
   @buffer BytePtr
   @length Int
   @max_length Int
end

class MyClass
   @host Str
end

def print(str Str)
   print_bytes(str.buffer, str.length)
end

def main
   hosto = "one"
   instance = MyClass.new(hosto)

   instance.host = "something new"

   print(instance.host)
end
