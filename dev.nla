class Str
   impl ToString
      def to_string() -> Str
         self
      end
   end
end

class Int
   impl ToString
      def to_string() -> Str
         system_cast_int_to_string(self)
      end
   end
end

def print(value ToString)
   system_print(value.to_string())
end

def main
   print(123)
end
