def_e print_int(int Int)
def_e print_bytes(byte_ptr BytePtr, int Int)

class Str
   @buffer BytePtr
   @length Int
   @max_length Int
end

def base_print(str Str)
   print_bytes(str.buffer, str.length)
end

class MyClass
   @host Str

   def agogo
      base_print(@host)
   end
end

def main
   instance = MyClass.new("localhost")
   instance.agogo()

   instance.host = "newhost"
   instance.agogo()
end
